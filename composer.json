{
  "name": "bitbag/sylius-agreement-plugin",
  "description": "Agreement plugin for Sylius",
  "type": "sylius-plugin",
  "license": "MIT",
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "BitBag\\SyliusAgreementPlugin\\": "src/",
      "Tests\\BitBag\\SyliusAgreementPlugin\\": ["tests/Application/src/", "tests/"]
    }
  },
  "require": {
    "sylius/sylius": "^1.9@stable || ^1.10@stable",
    "friendsofsymfony/oauth-server-bundle": "2.*@dev",
    "php": "^7.4 || ^8.0"
  },
  "require-dev": {

    "behat/mink": "^1.9",
    "behat/mink-extension": "^2.3",
    "behat/behat": "^3.10",
    "behat/mink-selenium2-driver": "^1.5",
    "dmore/behat-chrome-extension": "^1.3",
    "dmore/chrome-mink-driver": "^2.8",
    "friends-of-behat/mink-debug-extension": "^2.0",
    "friends-of-behat/mink-browserkit-driver": "^1.5",
    "friends-of-behat/page-object-extension": "^0.3",
    "friends-of-behat/suite-settings-extension": "^1.1",
    "friends-of-behat/symfony-extension": "^2.2",
    "friends-of-behat/variadic-extension": "^1.4",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-webmozart-assert": "^1.0",
    "phpunit/phpunit": "^9.5",
    "symfony/dotenv": "*",
    "symfony/config": "^2.7 | ^3.0 | ^4.0",
    "symfony/debug-bundle": "*",
    "symfony/web-profiler-bundle": "*",
    "symfony/browser-kit": "*",
    "vimeo/psalm": "4.4.1"
  }
}
