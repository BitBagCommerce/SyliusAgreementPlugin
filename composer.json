{
  "name": "bitbag/sylius-agreement-plugin",
  "description": "Agreement plugin for Sylius",
  "type": "sylius-plugin",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "BitBag\\SyliusAgreementPlugin\\": "src/",
      "BitBag\\SyliusAgreementPlugin\\Tests\\": "tests/",
      "Tests\\BitBag\\SyliusAgreementPlugin\\": "tests/Application/src/"
    }
  },
  "require": {
    "sylius/sylius": "^1.9 || ^1.10",
    "php": "^7.4 || ^8.0",
    "symfony/dotenv": "^5.3",
    "symfony/debug": "^4.4",
    "symfony/debug-bundle": "^5.3",
    "symfony/web-profiler-bundle": "^5.3",
    "sensiolabs/security-checker": "^6.0"
  },
  "require-dev": {
    "vimeo/psalm": "4.4.1",
    "phpunit/phpunit": "^9.5",
    "friends-of-behat/symfony-extension": "^2.0"
  }
}
